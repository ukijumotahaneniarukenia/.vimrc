[user]
	email = mrchildrenkh1008@gmail.com
	name = ukijumotahaneniarukenia
[core] #日本語の8進数表記を文字に変換する http://www-creators.com/archives/78
	pager = less -cm
	quotepath = false
	editor = vim
	excludesfile = /home/aine/.gitignore-system
	autocrlf = input
	whitespace = trailing-space,space-before-tab,indent-with-non-tab
[merge] #https://git-scm.com/book/ja/v2/Git-%E3%81%AE%E3%82%AB%E3%82%B9%E3%82%BF%E3%83%9E%E3%82%A4%E3%82%BA-Git-%E3%81%AE%E8%A8%AD%E5%AE%9A
	tool = gitmerge-p4merge-bash
[mergetool "gitmerge-p4merge-bash"]
	cmd = gitmerge-p4merge-bash "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
	trustExitCode = false
[diff]
	external = gitdiff-p4merge-bash
